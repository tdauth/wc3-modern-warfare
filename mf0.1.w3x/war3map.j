    if ( Trig_Start_Alliances_Func007Func001C() ) then
        call SetForceAllianceStateBJ( GetForceOfPlayer(GetEnumPlayer()), GetPlayersAllies(GetEnumPlayer()), bj_ALLIANCE_ALLIED_ADVUNITS )
    else
        call DoNothing(  )
    endif
endfunction

