function Trig_Start_AI_Func001Func001C takes nothing returns boolean
    if ( not ( GetPlayerSlotState(GetEnumPlayer()) == PLAYER_SLOT_STATE_PLAYING ) ) then
        return false
    endif
    return true
endfunction

function Trig_Start_AI_Func001A takes nothing returns nothing
    if ( Trig_Start_AI_Func001Func001C() ) then
        call StartCampaignAI( GetEnumPlayer(), "war3mapImported\\modernwarfare.ai" )
    else
        set bj_wantDestroyGroup = true
        call ForGroupBJ( GetUnitsOfPlayerAll(GetEnumPlayer()), function Trig_Start_AI_Func001Func001Func002A )
        call CustomDefeatBJ( GetEnumPlayer(), "TRIGSTR_109" )
    endif
endfunction

function Trig_Start_AI_Actions takes nothing returns nothing
